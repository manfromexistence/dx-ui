{
  "compilerOptions": {
    "target": "ES2017",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": [
        "./*"
      ],
      // "@/components/kokonutui/*": [
      //   "./components/kokonutui/block/*",
      //   "./components/kokonutui/pricing/*",
      //   "./components/kokonutui/alert/*",
      //   "./components/kokonutui/button/*",
      //   "./components/kokonutui/input/*",
      //   "./components/kokonutui/card/*",
      //   "./components/kokonutui/text/*",
      //   "./components/kokonutui/ai-input/*",
      //   "./components/kokonutui/faq/*",
      //   "./components/kokonutui/list/*",
      //   "./components/kokonutui/profile/*"
      // ]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts"
, "next.config.mjs", "content/docs/components/cards.mdx", "content/docs/components/faq.mdx"  ],
  "exclude": [
    "node_modules"
  ]
}